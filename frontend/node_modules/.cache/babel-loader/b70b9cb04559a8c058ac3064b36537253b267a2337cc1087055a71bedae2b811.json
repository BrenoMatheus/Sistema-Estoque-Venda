{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport VendasService from '../../services/api/vendas/VendasService';\nimport DashboardComponent from '../Dashboard/DashboardComponent';\nimport SelectGrupoComponent from './components/SelectGrupoComponent';\nimport SelectBrandComponent from './components/SelectBrandComponent';\nexport default {\n  name: 'SaleListComponent',\n  components: {\n    DashboardComponent,\n    SelectGrupoComponent,\n    SelectBrandComponent\n  },\n  data() {\n    return {\n      searchQuery: '',\n      selectedGroup: '',\n      selectedBrand: '',\n      handlePages: '',\n      selectedStatus: '',\n      sale: [],\n      arrayPage: [],\n      paginate: {},\n      minSearchLength: 3,\n      currentPage: 1,\n      itemsPerPage: 10\n    };\n  },\n  computed: {},\n  methods: {\n    formatDate(dateString) {\n      const date = new Date(dateString);\n      return date.toLocaleDateString('pt-BR');\n    },\n    formatCurrency(value) {\n      return value.toLocaleString('pt-BR', {\n        style: 'currency',\n        currency: 'BRL'\n      });\n    },\n    editar(id) {\n      this.$router.push(`/venda/detalhes/${id}`);\n    },\n    cadastrar() {\n      this.$router.push(`/venda/detalhes/nova`);\n    },\n    async remover(id) {\n      try {\n        await VendasService.remover(id);\n        console.log('Venda removida com sucesso!');\n        this.getAll();\n      } catch (error) {\n        console.error('Erro ao remover venda:', error);\n      }\n    },\n    async getAll() {\n      try {\n        const params = this.buildParams();\n        const response = await VendasService.getAll(params);\n        this.$router.push({\n          path: `/venda`,\n          query: params\n        });\n        this.sale = response.data;\n        this.paginate = response.last_page;\n        if (this.paginate > 1) {\n          this.arrayPage = this.arrayPages(this.paginate);\n        }\n      } catch (error) {\n        console.error('Erro ao obter vendas
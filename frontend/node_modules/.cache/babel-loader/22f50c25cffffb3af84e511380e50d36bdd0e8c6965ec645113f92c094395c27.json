 '',\n      selectedGender: '',\n      handlePages: '',\n      selectedAge: '',\n      people: [],\n      arrayPage: [],\n      paginate: {},\n      minSearchLength: 3,\n      currentPage: 1,\n      itemsPerPage: 10,\n    };\n  },\n  computed: {},\n  methods: {\n    editar(id) {\n      this.$router.push(`/pessoa/detalhes/${id}`);\n    },\n    cadastrar() {\n      this.$router.push(`/pessoa/detalhes/nova`);\n    },\n    async remover(id) {\n      try {\n        await PessoasService.remover(id);\n        console.log('Pessoa removida com sucesso!');\n        this.getAll();\n      } catch (error) {\n        console.error('Erro ao remover pessoa:', error);\n      }\n    },\n    async getAll() {\n      try {\n        const params = this.buildParams();\n        const response = await PessoasService.getAll(params);\n        this.$router.push({ path: `/pessoa`, query: params });\n        this.people = response.data;\n        this.paginate = response.last_page;\n        if (this.paginate > 1) {\n          this.arrayPage = this.arrayPages(this.paginate);\n        }\n      } catch (error) {\n        console.error('Erro ao obter pessoas', error);\n        this.people = [];\n        this.paginate = [];\n      }\n    },\n\n    shouldApplyFilter() {\n      return (\n        this.searchQuery.length >= this.minSearchLength ||\n        this.selectedGroup ||\n        this.selectedGender ||\n        this.selectedGender ||\n        this.selectedAge\n      );\n    },\n\n    buildParams() {\n      const params = {};\n      if (this.selectedGroup) {\n        params.grupo_id = this.selectedGroup;\n      }\n      if (this.selectedGender) {\n        params.genero = this.selectedGender;\n      }\n      if (this.searchQuery.length >= this.minSearchLength) {\n        params.nome = this.searchQuery;\n      }\n      if (this.selectedAge) {\n        params.idade = this.selectedAge;\n      }\n      if (this.handlePages) {\n        params.page = this.handlePages;\n      }\n      return params;\n    },\n\n    updateSelectedGroup(groupId) {\n      this.selectedGroup = groupId;\n      this.handleFilterChange();\n    },\n    handleFilterChange() {\n      if (this.shouldApplyFilter()) {\n        this.currentPage = 1;\n      } else {\n        this.getAll();\n      }\n    },\n    arrayPages(lastPage) {\n      let pages = [];\n      for (let i = 1; i <= lastPage; i++) {\n        pages.push(i);\n      }\n\n      return pages;\n    },\n\n    handlePage(url) {\n      if (url) {\n        this.handlePages = url;\n        this.getAll();\n      }\n    },\n  },\n  watch: {\n    searchQuery() {\n      if (this.shouldApplyFilter()) {\n        this.getAll();\n      }\n    },\n    selectedGroup() {\n      if (this.shouldApplyFilter()) {\n        this.getAll();\n      }\n    },\n    selectedGender() {\n      if (this.shouldApplyFilter()) {\n        this.getAll();\n      }\n    },\n    selectedAge() {\n      if (this.shouldApplyFilter()) {\n        this.getAll();\n      }\n    },\n    currentPage() {\n      this.getAll();\n    },\n  },\n  mounted() {\n    this.getAll();\n  },\n};\n</script>\n"],"mappings":";AAoKA,OAAOA,cAAa,MAAO,2CAA2C;AACtE,OAAOC,kBAAiB,MAAO,iCAAiC;AAChE,OAAOC,oBAAmB,MAAO,mCAAmC;AAEpE,eAAe;EACbC,IAAI,EAAE,qBAAqB;EAC3BC,UAAU,EAAE;IACVH,kBAAkB;IAClBC;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,aAAa,EAAE,EAAE;MACjBC,cAAc,EAAE,EAAE;MAClBC,WAAW,EAAE,EAAE;MACfC,WAAW,EAAE,EAAE;MACfC,MAAM,EAAE,EAAE;MACVC,SAAS,EAAE,EAAE;MACbC,QAAQ,EAAE,CAAC,CAAC;MACZC,eAAe,EAAE,CAAC;MAClBC,WAAW,EAAE,CAAC;MACdC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE,CAAC,CAAC;EACZC,OAAO,EAAE;IACPC,MAAMA,CAACC,EAAE,EAAE;MACT,IAAI,CAACC,OAAO,CAACC,IAAI,CAAE,oBAAmBF,EAAG,EAAC,CAAC;IAC7C,CAAC;IACDG,SAASA,CAAA,EAAG;MACV,IAAI,CAACF,OAAO,CAACC,IAAI,CAAE,uBAAsB,CAAC;IAC5C,CAAC;IACD,MAAME,OAAOA,CAACJ,EAAE,EAAE;MAChB,IAAI;QACF,MAAMpB,cAAc,CAACwB,OAAO,CAACJ,EAAE,CAAC;QAChCK,OAAO,CAACC,GAAG,CAAC,8BAA8B,CAAC;QAC3C,IAAI,CAACC,MAAM,CAAC,CAAC;MACf,EAAE,OAAOC,KAAK,EAAE;QACdH,OAAO,CAACG,KAAK,CA
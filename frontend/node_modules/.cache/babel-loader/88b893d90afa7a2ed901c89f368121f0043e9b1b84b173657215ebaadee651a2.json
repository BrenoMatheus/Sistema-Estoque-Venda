{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport PessoasService from '../../services/api/pessoas/PessoasService';\nimport DashboardComponent from '../Dashboard/DashboardComponent';\nimport SelectGrupoComponent from './components/SelectGrupoComponent';\nexport default {\n  name: 'ListComponent',\n  components: {\n    DashboardComponent,\n    SelectGrupoComponent\n  },\n  data() {\n    return {\n      searchQuery: '',\n      selectedGroup: '',\n      selectedGender: '',\n      selectedAge: '',\n      people: [],\n      paginate: {},\n      minSearchLength: 3,\n      currentPage: 1,\n      itemsPerPage: 10\n    };\n  },\n  computed: {\n    filteredPeople() {\n      let filtered = this.people;\n      let paginate = this.paginate;\n      if (this.shouldApplyFilters()) filtered = this.applyFilters(filtered);\n      return filtered, paginate;\n    },\n    totalPages() {\n      return Math.ceil(this.filteredPeople.length / this.itemsPerPage);\n    },\n    startIndex() {\n      return (this.currentPage - 1) * this.itemsPerPage;\n    },\n    endIndex() {\n      return Math.min(this.startIndex + this.itemsPerPage, this.filteredPeople.length);\n    },\n    paginatedPeople() {\n      return this.filteredPeople.slice(this.startIndex, this.endIndex);\n    }\n  },\n  methods: {\n    navigateToPage(pageNumber) {\n      this.currentPage = pageNumber;\n    },\n    editar(id) {\n      this.$router.push(`/pessoa/detalhes/${id}`);\n    },\n    cadastrar() {\n      this.$router.push(`/pessoa/detalhes/nova`);\n    },\n    async remover(id) {\n      try {\n        await PessoasService.remover(id);\n        console.log('Pessoa removida com sucesso!');\n        this.getAll();\n      } catch (error) {\n        console.error('Erro ao remover pessoa:', error);\n      }\n    },\n    async getAll() {\n      try {\n        const params = this.buildParams();\n        const response = await PessoasService.getAll(params);\n        this.people = response.data;\n      } catch (error) {\n        console.error('Erro ao obter pessoas:', error);\n        this.people = [];\n      }\n    },\n    shouldApplyFilters() {\n      return this.searchQuery.length >= this.minSearchLength || this.selectedGroup || this.selectedGender || this.selectedAge;\n    },\n    applyFilters(data) {\n      let filtered = data;\n      if (this.searchQuery.length >= this.minSearchLength) {\n        filtered = filtered.filter(person => person.name.toLowerCase().includes(this.searchQuery.toLowerCase()));\n      }\n      if (this.selectedGroup) {\n        filtered = filtered.filter(person => person.group === this.selectedGroup);\n      }\n      if (this.selectedGender) {\n        filtered = filtered.filter(person => person.gender === this.selectedGender);\n      }\n      if (this.selectedAge) {\n        filtered = filtered.filter(person => person.age === parseInt(this.selectedAge));\n      }\n      return filtered;\n    },\n    buildParams() {\n      const params = {};\n      if (this.selectedGroup) {\n        params.grupo_id = this.selectedGroup;\n      }\n      if (this.selectedGender) {\n        params.genero = this.selectedGender;\n      }\n      if (this.searchQuery.length >= this.minSearchLength) {\n        params.nome = this.searchQuery;\n      }\n      if (this.selectedAge) {\n        params.idade = this.selectedAge;\n      }\n      return params;\n    },\n    updateSelectedGroup(groupId) {\n      this.selectedGroup = groupId;\n      this.handleFilterChange();\n    },\n    handleFilterChange() {\n      if (this.shouldApplyFilters()) {\n        this.currentPage = 1;\n      } else {\n        this.getAll();\n      }\n    },\n    previousPage() {\n      if (this.currentPage > 1) {\n        this.currentPage--;\n      }\n    },\n    nextPage() {\n      if (this.currentPage < this.totalPages) {\n        this.currentPage++;\n      }\n    }\n  },\n  watch: {\n    searchQuery() {\n      if (this.shouldApplyFilters()) {\n        this.getAll();\n      }\n    },\n    selectedGroup() {\n      if (this.shouldApplyFilters()) {\n        this.getAll();\n      }\n    },\n    selectedGender() {\n      if (this.shouldApplyFilters()) {\n        this.getAll();\n      }\n    },\n    selectedAge() {\n      if (this.shouldApplyFilters()) {\n        this.getAll();\n      }\n    },\n    currentPage() {\n      this.getAll();\n    }\n  },\n  mounted() {\n    this.getAll();\n  }\n};","map":{"version":3,"names":["PessoasService","DashboardComponent","SelectGrupoComponent","name","components","data","searchQuery","selectedGroup","selectedGender","selectedAge","people","paginate","minSearchLength","currentPage","itemsPerPage","computed","filteredPeople","filtered","shouldApplyFilters","applyFilters","totalPages","Math","ceil","length","startIndex","endIndex","min","paginatedPeople","slice","methods","navigateToPage","pageNumber","editar","id","$router","push","cadastrar","remover","console","log","getAll","error","params","buildParams","response","filter","person","toLowerCase","includes","group","gender","age","parseInt","grupo_id","genero","nome","idade","updateSelectedGroup","groupId","handleFilterChange","previousPage","nextPage","watch","mounted"],"sources":["/home/brenomatheus/Documentos/Teste Pratico DEV Junior/frontend/src/pages/Pessoa/ListComponent.vue"],"sourcesContent":["<template>\n  <DashboardComponent>\n    <template v-slot:slot-pages>\n      <h5\n        class=\"text-2xl font-medium text-gray-900 dark:text-white text-center border-b-2 m-2 p-4 border-gray-900\"\n      >\n        Pessoas\n      </h5>\n      <div class=\"flex justify-center items-center\">\n        <div class=\"w-full p-4 sm:p-6 md:p-8 dark:bg-gray-800\">\n          <div class=\"w-auto\">\n            <div class=\"flex justify-center\">\n              <button\n                type=\"submit\"\n                class=\"w-30 text-white bg-blue-700 hover:bg-blue-800 focus:ring-4 focus:outline-none focus:ring-blue-300 font-medium rounded-lg text-sm px-5 py-2.5 text-center dark:bg-blue-60
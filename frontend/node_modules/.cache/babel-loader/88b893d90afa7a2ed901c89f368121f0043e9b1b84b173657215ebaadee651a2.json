  if (this.searchQuery.length >= this.minSearchLength) {\n        filtered = filtered.filter(person =>\n          person.name.toLowerCase().includes(this.searchQuery.toLowerCase())\n        );\n      }\n      if (this.selectedGroup) {\n        filtered = filtered.filter(\n          person => person.group === this.selectedGroup\n        );\n      }\n      if (this.selectedGender) {\n        filtered = filtered.filter(\n          person => person.gender === this.selectedGender\n        );\n      }\n      if (this.selectedAge) {\n        filtered = filtered.filter(\n          person => person.age === parseInt(this.selectedAge)\n        );\n      }\n      return filtered;\n    },\n    buildParams() {\n      const params = {};\n      if (this.selectedGroup) {\n        params.grupo_id = this.selectedGroup;\n      }\n      if (this.selectedGender) {\n        params.genero = this.selectedGender;\n      }\n      if (this.searchQuery.length >= this.minSearchLength) {\n        params.nome = this.searchQuery;\n      }\n      if (this.selectedAge) {\n        params.idade = this.selectedAge;\n      }\n      return params;\n    },\n    updateSelectedGroup(groupId) {\n      this.selectedGroup = groupId;\n      this.handleFilterChange();\n    },\n    handleFilterChange() {\n      if (this.shouldApplyFilters()) {\n        this.currentPage = 1;\n      } else {\n        this.getAll();\n      }\n    },\n    previousPage() {\n      if (this.currentPage > 1) {\n        this.currentPage--;\n      }\n    },\n    nextPage() {\n      if (this.currentPage < this.totalPages) {\n        this.currentPage++;\n      }\n    },\n  },\n  watch: {\n    searchQuery() {\n      if (this.shouldApplyFilters()) {\n        this.getAll();\n      }\n    },\n    selectedGroup() {\n      if (this.shouldApplyFilters()) {\n        this.getAll();\n      }\n    },\n    selectedGender() {\n      if (this.shouldApplyFilters()) {\n        this.getAll();\n      }\n    },\n    selectedAge() {\n      if (this.shouldApplyFilters()) {\n        this.getAll();\n      }\n    },\n    currentPage() {\n      this.getAll();\n    },\n  },\n  mounted() {\n    this.getAll();\n  },\n};\n</script>"],"mappings":";AAwIA,OAAOA,cAAa,MAAO,2CAA2C;AACtE,OAAOC,kBAAiB,MAAO,iCAAiC;AAChE,OAAOC,oBAAmB,MAAO,mCAAmC;AAEpE,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,UAAU,EAAE;IACVH,kBAAkB;IAClBC;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,aAAa,EAAE,EAAE;MACjBC,cAAc,EAAE,EAAE;MAClBC,WAAW,EAAE,EAAE;MACfC,MAAM,EAAE,EAAE;MACVC,QAAQ,EAAE,CAAC,CAAC;MACZC,eAAe,EAAE,CAAC;MAClBC,WAAW,EAAE,CAAC;MACdC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,cAAcA,CAAA,EAAG;MACf,IAAIC,QAAO,GAAI,IAAI,CAACP,MAAM;MAC1B,IAAIC,QAAO,GAAI,IAAI,CAACA,QAAQ;MAC5B,IAAI,IAAI,CAACO,kBAAkB,CAAC,CAAC,EAAED,QAAO,GAAI,IAAI,CAACE,YAAY,CAACF,QAAQ,CAAC;MACrE,OAAOA,QAAQ,EAAEN,QAAQ;IAC3B,CAAC;IACDS,UAAUA,CAAA,EAAG;MACX,OAAOC,IAAI,CAACC,IAAI,CAAC,IAAI,CAACN,cAAc,CAACO,MAAK,GAAI,IAAI,CAACT,YAAY,CAAC;IAClE,CAAC;IACDU,UAAUA,CAAA,EAAG;MACX,OAAO,CAAC,IAAI,CAACX,WAAU,GAAI,CAAC,IAAI,IAAI,CAACC,YAAY;IACnD,CAAC;IACDW,QAAQA,CAAA,EAAG;MACT,OAAOJ,IAAI,CAACK,GAAG,CAAC,IAAI,CAACF,UAAS,GAAI,IAAI,CAACV,YAAY,EAAE,IAAI,CAACE,cAAc,CAACO,MAAM,CAAC;IAClF,CAAC;IACDI,eAAeA,CAAA,EAAG;MAChB,OAAO,IAAI,CAACX,cAAc,CAACY,KAAK,CAAC,IAAI,CAACJ,UAAU,EAAE,IAAI,CAACC,QAAQ,CAAC;IAClE;EACF,CAAC;EACDI,OAAO,EAAE;IACPC,cAAcA,CAACC,UAAU,EAAE;MACzB,IAAI,CAAClB,WAAU,GAAIkB,UAAU;IAC/B,CAAC;IACDC,MAAMA,CAACC,EAAE,EAAE;MACT,IAAI,CAACC,OAAO,CAACC,IAAI,CAAE,oBAAmBF,EAAG,EAAC,CAAC;IAC7C,CAAC;IACDG,SAASA,CAAA,EAAG;MACV,IAAI,CAACF,OAAO,CAACC,IAAI,CAAE,uBAAsB,CAAC;IAC5C,CAAC;IACD,MAAME,OAAOA,CAACJ,EAAE,EAAE;MAChB,IAAI;QACF,MAAMjC,cAAc,CAACqC,OAAO,CAACJ,EAAE,CAAC;QAChCK,OAAO,CAACC,GAAG,CAAC,8BAA8B,CAAC;QAC3C,IAAI,CAACC,MAAM,CAAC,CAAC;MACf,EAAE,OAAOC,KAAK,EAAE;QACdH,OAAO,CAACG,KAAK,CAAC,yBAAyB,EAAEA,KAAK,CAAC;MACjD;IACF,CAAC;IACD,MAAMD,MAAMA,CAAA,EAAG;MACb,IAAI;QACF,MAAME,MAAK,GAAI,IAAI,CAACC,WAAW,CAAC,CAAC;QACjC,MAAMC,QAAO,GAAI,MAAM5C,cAAc,CAACwC,MAAM,CAACE,MAAM,CAAC;QACpD,IAAI,CAAChC,MAA
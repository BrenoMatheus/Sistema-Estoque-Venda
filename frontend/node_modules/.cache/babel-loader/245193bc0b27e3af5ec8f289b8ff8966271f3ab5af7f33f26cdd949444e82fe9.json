{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport PessoasService from '../../services/api/pessoas/PessoasService';\nimport DashboardComponent from '../Dashboard/DashboardComponent';\nimport SelectGrupoComponent from './components/SelectGrupoComponent';\nexport default {\n  name: 'ListComponent',\n  components: {\n    DashboardComponent,\n    SelectGrupoComponent\n  },\n  data() {\n    return {\n      searchQuery: '',\n      selectedGroup: '',\n      selectedGender: '',\n      handlePages: '',\n      selectedAge: '',\n      people: [],\n      arrayPage: [],\n      paginate: {},\n      minSearchLength: 3,\n      currentPage: 1,\n      itemsPerPage: 10\n    };\n  },\n  computed: {\n    pages() {\n      return this.paginate.links ? this.paginate.links : [];\n    },\n    totalPages() {\n      return this.paginate.last_page;\n    },\n    startIndex() {\n      return (this.paginate.current_page - 1) * this.itemsPerPage;\n    },\n    endIndex() {\n      return Math.min(this.startIndex + this.itemsPerPage, this.paginate.total);\n    },\n    paginatedPeople() {\n      return this.paginate.data ? this.paginate.data.slice(this.startIndex, this.endIndex) : [];\n    }\n  },\n  methods: {\n    editar(id) {\n      this.$router.push(`/pessoa/detalhes/${id}`);\n    },\n    cadastrar() {\n      this.$router.push(`/pessoa/detalhes/nova`);\n    },\n    async remover(id) {\n      try {\n        await PessoasService.remover(id);\n        console.log('Pessoa removida com sucesso!');\n        this.getAll();\n      } catch (error) {\n        console.error('Erro ao remover pessoa:', error);\n      }\n    },\n    async getAll() {\n      try {\n        const params = this.buildParams();\n        //const pageUrl = this.buildParams();\n        const response = await PessoasService.getAll(params);\n        this.$router.push({\n          path: `/pessoa`,\n          query: params\n        });\n        this.people = response.data;\n        console.log(response.last_page);\n        this.paginate = response.last_page;\n        if (this.paginate > 1) {\n          this.arrayPage = await arrayPages(this.paginate);\n        }\n      } catch (error) {\n        console.error('Erro ao obter pessoas', error);\n        this.people = [];\n        this.paginate = [];\n      }\n    },\n    shouldApplyFilter() {\n      return this.searchQuery.length >= this.minSearchLength || this.selectedGroup || this.selectedGender || this.selectedGender || this.selectedAge;\n    },\n    shouldApplyPage(page) {\n      return this.selectedPages = page;\n    },\n    buildParams() {\n      const params = {};\n      if (this.selectedGroup) {\n        params.grupo_id = this.selectedGroup;\n      }\n      if (this.selectedGender) {\n        params.genero = this.selectedGender;\n      }\n      if (this.searchQuery.length >= this.minSearchLength) {\n        params.nome = this.searchQuery;\n      }\n      if (this.selectedAge) {\n        params.idade = this.selectedAge;\n      }\n      if (this.handlePages) {\n        params.page = this.handlePages;\n      }\n      return params;\n    },\n    updateSelectedGroup(groupId) {\n      this.selectedGroup = groupId;\n      this.handleFilterChange();\n    },\n    handleFilterChange() {\n      if (this.shouldApplyFilter()) {\n        this.currentPage = 1;\n      } else {\n        this.getAll();\n      }\n    },\n    arrayPages(lastPage) {\n      let pages = [];\n      for (let i = 1; i <= lastPage; i++) {\n        pages.push(i);\n      }\n      return pages;\n    },\n    handlePage(url) {\n      console.log(url);\n      if (url) {\n        this.handlePages = url;\n        this.getAll();\n      }\n    }\n  },\n  watch: {\n    searchQuery() {\n      if (this.shouldApplyFilter()) {\n        this.getAll();\n      }\n    },\n    selectedGroup() {\n      if (this.shouldApplyFilter()) {\n        this.getAll();\n      }\n    },\n    selectedGender() {\n      if (this.shouldApplyFilter()) {\n        this.getAll();\n      }\n    },\n    selectedPages() {\n      if (this.shouldApplyPage()) {\n        this.getAll();\n      }\n    },\n    selectedAge() {\n      if (this.shouldApplyFilter()) {\n        this.getAll();\n      }\n    },\n    currentPage() {\n      this.getAll();\n    }\n  },\n  mounted() {\n    this.getAll();\n  }\n};","map":{"version":3,"names":["PessoasService","DashboardComponent","SelectGrupoComponent","name","components","data","searchQuery","selectedGroup","selectedGender","handlePages","selectedAge","people","arrayPage","paginate","minSearchLength","currentPage","itemsPerPage","computed","pages","links","totalPages","last_page","startIndex","current_page","endIndex","Math","min","total","paginatedPeople","slice","methods","editar","id","$router","push","cadastrar","remover","console","log","getAll","error","params","buildParams","response","path","query","arrayPages","shouldApplyFilter","length","shouldApplyPage","page","selectedPages","grupo_id","genero","nome","idade","updateSelectedGroup","groupId","handleFilterChange","lastPage","i","handlePage","url","watch","mounted"],"sources":["/home/brenomatheus/Documentos/Teste Pratico DEV Junior/frontend/src/pages/Pessoa/ListComponent.vue"],"sourcesContent":["<template>\n  <DashboardComponent>\n    <template v-slot:slot-pages>\n      <h5\n        class=\"text-2xl font-medium text-gray-900 dark:text-white text-center border-b-2 m-2 p-4 border-gray-900\"\n      >\n        Pessoas\n      </h5>\n      <div class=\"flex justify-center items-center\">\n        <div class=\"w-full p-4 sm:p-6 md:p-8 dark:bg-gray-800\">\n          <div class=\"w-auto\">\n            <div class=\"flex justify-center\">\n              <button\n                type=\"submit\"\n                class=\"w-30 text-white bg-blue-700 hover:bg-blue-800 focus:ring-4 focus:outline-none focus:ring-blue-300 font-medium rounded-lg text-sm px-5 py-2.5 text-center dark:bg-blue-600 dark:hover:bg-blue-700 dark:focus:ring-blue-800\"\n                @click=\"cadastrar()\"\n              >\n                Cadastrar\n              </button>\n            </div>\n            <div class=\"mb-4 flex items-center justify-between\">\n              <input\n                type=\"text\"\n                v-model=\"searchQuery\"\n                class=\"py-2 px-3 w-1/3 rounded border\"\n                placeholder=\"Pesquisar...\"\n                @input=\"handleFilterChange\"\n              />\n              <span class=\"border rounded py-3 px-3 bg-slate-300\">\n                <i class=\"fa-solid fa-magnifying-glass\"></i>\n              </span>\n            </div>\n            <div class=\"flex items-center justify-between mb-4\">\n              <SelectGrupoComponent @group-selected=\"updateSelectedGroup\" />\n\n              <select\n                v-model=\"selectedGender\"\n                class=\"py-2 px-3 rounded border mr-4\"\n                @change=\"handleFilterChange\"\n              >\n                <option value=\"\" selected>Todos os gêneros</option>\n                <option value=\"Masculino\">Masculino</option>\n                <option value=\"Feminino\">Feminino</option>\n                <option value=\"other\">Outro</option>\n              </select>\n              <input\n                type=\"number\"\n                v-model=\"selectedAge\"\n                class=\"py-2 px-3 rounded border mr-4\"\n                placeholder=\"Idade\"\n                @input=\"handleFilterChange\"\n              />\n            </div>\n          </div>\n          <div class=\"overflow-x-auto h-52 sm:h-56 md:h-64\">\n            <div class=\"relative shadow-md sm:rounded-lg max-h-screen\">\n              <table\n                class=\"w-full text-sm text-left overflow-y-auto rtl:text-right text-gray-500 dark:text-gray-400\"\n              >\n                <thead\n                  class=\"text-xs text-gray-700 uppercase bg-gray-50 dark:bg-gray-700 dark:text-gray-400\"\n                >\n                  <tr>\n                    <th scope=\"col\" class=\"px-6 py-3\">Nome</th>\n                    <th scope=\"col\" class=\"px-6 py-3\">Gênero</th>\n                    <th scope=\"col\" class=\"px-6 py-3\">Idade</th>\n                    <th scope=\"col\" class=\"px-6 py-3\">Grupo</th>\n                    <th scope=\"col\" class=\"px-6 py-3\">Ação</th>\n                  </tr>\n                </thead>\n                <tbody>\n                  <template v-if=\"!people.length\">\n                    <tr>\n                      <td colspan=\"5\" class=\"text-center\">\n                        Nenhum resultado encontrado.\n                      </td>\n                    </tr>\n                  </template>\n                  <template v-else>\n                    <tr\n                      v-for=\"row in people\"\n                      :key=\"row.id\"\n                      class=\"odd:bg-white odd:dark:bg-gray-900 even:bg-gray-50 even:dark:bg-gray-800 border-b dark:border-gray-700\"\n                    >\n                      <th\n                        scope=\"row\"\n                        class=\"px-6 py-4 font-medium text-gray-900 whitespace-nowrap dark:text-white\"\n                      >\n                        {{ row.nome }}\n                      </th>\n                      <td class=\"px-6 py-4\">{{ row.genero }}</td>\n                      <td class=\"px-6 py-4\">{{ row.idade }}</td>\n                      <td class=\"px-6 py-4\">{{ row.grupo_pessoa.nome }}</td>\n                      <td class=\"px-6 py-4\">\n                        <a\n                          @click=\"editar(row.pessoa.id)\"\n                          class=\"text-blue-600 pr-4 hover:text-white dark:text-blue-500 hover:underline hover:bg-blue-800 focus:ring-4 focus:outline-none focus:ring-blue-300 font-medium rounded-lg text-sm px-5 py-2.5 text-center dark:bg-blue-600 dark:hover:bg-blue-700 dark:focus:ring-blue-800\"\n                        >\n                          <i class=\"text-xl fa-solid fa-eye\"></i>\n                        </a>\n                        <a\n                          @click=\"remover(row.pessoa.id)\"\n                          class=\"text-blue-600 hover:text-white dark:text-blue-500 hover:underline hover:bg-blue-800 focus:ring-4 focus:outline-none focus:ring-blue-300 font-medium rounded-lg text-sm px-5 py-2.5 text-center dark:bg-blue-600 dark:hover:bg-blue-700 dark:focus:ring-blue-800\"\n                        >\n                          <i class=\"text-xl fa-solid fa-trash\"></i>\n                        </a>\n                      </td>\n                    </tr>\n                  </template>\n                </tbody>\n              </table>\n              <div class=\"flex justify-center mt-4\">\n                <template v-if=\"!pages.length\">\n                  \n                </template>\n                <template v-else>\n                  <a\n                    v-for=\"link in pages\"\n                    :key=\"link\"\n                    :v-html=\"link\" \n                    @click=\"handlePage(link)\"\n                    class=\"odd:bg-white odd:dark:bg-gray-900 even:bg-gray-50 even:dark:bg-gray-800 border-b dark:border-gray-700 p-4\"\n                  >{{link}}</a>\n                </template>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </template>\n  </DashboardComponent>\n</template>\n\n<script>\nimport PessoasService from '../../services/api/pessoas/PessoasService';\nimport DashboardComponent from '../Dashboard/DashboardComponent';\nimport SelectGrupoComponent from './components/SelectGrupoComponent';\n\nexport default {\n  name: 'ListComponent',\n  components: {\n    DashboardComponent,\n    SelectGrupoComponent,\n  },\n  data() {\n    return {\n      searchQuery: '',\n      selectedGroup: '',\n      selectedGender: '',\n      handlePages: '',\n      selectedAge: '',\n      people: [],\n      arrayPage:[],\n      paginate: {},\n      minSearchLength: 3,\n      currentPage: 1,\n      itemsPerPage: 10,\n    };\n  },\n  computed: {\n \n    pages() {\n      return this.paginate.links\n        ? this.paginate.links\n        : [];\n    },\n    totalPages() {\n      return this.paginate.last_page;\n    },\n    startIndex() {\n      return (this.paginate.current_page - 1) * this.itemsPerPage;\n    },\n    endIndex() {\n      return Math.min(this.startIndex + this.itemsPerPage, this.paginate.total);\n    },\n    paginatedPeople() {\n\n      return this.paginate.data\n        ? this.paginate.data.slice(this.startIndex, this.endIndex)\n        : [];\n    },\n  },\n  methods: {\n    editar(id) {\n      this.$router.push(`/pessoa/detalhes/${id}`);\n    },\n    cadastrar() {\n      this.$router.push(`/pessoa/detalhes/nova`);\n    },\n    async remover(id) {\n      try {\n        await PessoasService.remover(id);\n        console.log('Pessoa removida com sucesso!');\n        this.getAll();\n      } catch (error) {\n        console.error('Erro ao remover pessoa:', error);\n      }\n    },\n    async getAll() {\n      try {\n        const params = this.buildParams();\n        //const pageUrl = this.buildParams();\n        const response = await PessoasService.getAll(params)\n        this.$router.push({ path: `/pessoa`, query: params });\n        this.people = response.data;\n        console.log(response.last_page);\n        this.paginate = response.last_page;\n        if(this.paginate > 1) {\n          this.arrayPage = await arrayPages(this.paginate)\n        }\n      } catch (error) {\n        console.error('Erro ao obter pessoas', error);\n        this.people = [];\n        this.paginate = [];\n      }\n    },\n    \n    shouldApplyFilter() {\n      return (\n        this.searchQuery.length >= this.minSearchLength ||\n        this.selectedGroup ||\n        this.selectedGender ||\n        this.selectedGender ||\n        this.selectedAge\n      );\n    },\n    shouldApplyPage(page) {\n      return (\n        this.selectedPages = page\n      );\n    },\n    \n    buildParams() {\n      const params = {};\n      if (this.selectedGroup) {\n        params.grupo_id = this.selectedGroup;\n      }\n      if (this.selectedGender) {\n        params.genero = this.selectedGender;\n      }\n      if (this.searchQuery.length >= this.minSearchLength) {\n        params.nome = this.searchQuery;\n      }\n      if (this.selectedAge) {\n        params.idade = this.selectedAge;\n      }\n      if (this.handlePages) {\n        params.page = this.handlePages ;\n      }\n      return params;\n    },\n\n\n    updateSelectedGroup(groupId) {\n      this.selectedGroup = groupId;\n      this.handleFilterChange();\n    },\n    handleFilterChange() {\n      if (this.shouldApplyFilter()) {\n        this.currentPage = 1;\n      } else {\n        this.getAll();\n      }\n    },\n     arrayPages(lastPage) {\n      let pages = [];\n      for (let i = 1; i <= lastPage; i++) {\n        pages.push(i);\n      }\n\n  return pages;\n},\n\n    handlePage(url) {\n      console.log(url)\n      if(url){\n        this.handlePages = url\n        this.getAll();\n      }\n        \n    },\n  },\n  watch: {\n    searchQuery() {\n      if (this.shouldApplyFilter()) {\n        this.getAll();\n      }\n    },\n    selectedGroup() {\n      if (this.shouldApplyFilter()) {\n        this.getAll();\n      }\n    },\n    selectedGender() {\n      if (this.shouldApplyFilter()) {\n\n        this.getAll();\n      }\n    },\n    selectedPages() {\n      if (this.shouldApplyPage()) {\n        this.getAll();\n      }\n    },\n    selectedAge() {\n      if (this.shouldApplyFilter()) {\n        this.getAll();\n      }\n    },\n    currentPage() {\n        this.getAll();\n    },\n  },\n  mounted() {\n    this.getAll();\n  },\n};\n</script>\n"],"mappings":";AAsIA,OAAOA,cAAa,MAAO,2CAA2C;AACtE,OAAOC,kBAAiB,MAAO,iCAAiC;AAChE,OAAOC,oBAAmB,MAAO,mCAAmC;AAEpE,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,UAAU,EAAE;IACVH,kBAAkB;IAClBC;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,aAA
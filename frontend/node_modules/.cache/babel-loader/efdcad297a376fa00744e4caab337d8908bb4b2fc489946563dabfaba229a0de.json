his.searchQuery.length >= this.minSearchLength || this.selectedGroup || this.selectedGender || this.selectedAge) {\n        this.getAll();\n      } else {\n        // Se não, limpa a lista de pessoas\n        this.people = [];\n      }\n    },\n    handleFilterChange() {\n      // Chama getAll apenas se pelo menos um filtro estiver selecionado\n      if (this.searchQuery.length >= this.minSearchLength || this.selectedGroup || this.selectedGender || this.selectedAge) {\n        this.getAll();\n      } else {\n        // Se não, limpa a lista de pessoas\n        this.people = [];\n      }\n    }\n  },\n  created() {\n    // this.getAll(); // Não é necessário chamar getAll no created\n  },\n  mounted() {\n    if (this.people.length === 0) {\n      this.getAll();\n    }\n  }\n};","map":{"version":3,"names":["PessoasService","DashboardComponent","name","components","data","searchQuery","selectedGroup","selectedGender","selectedAge","people","minSearchLength","computed","filteredPeople","filtered","length","filter","person","toLowerCase","includes","group","gender","age","parseInt","methods","editar","id","$router","push","remover","console","log","getAll","error","response","Error","handleSearchInput","handleFilterChange","created","mounted"],"sources":["/home/brenomatheus/Documentos/Teste Pratico DEV Junior/frontend/src/pages/Pessoa/ListComponent.vue"],"sourcesContent":["<template>\n  <DashboardComponent>\n    <template v-slot:slot-pages>\n      <h5 class=\"text-2xl font-medium text-gray-900 dark:text-white text-center border-b-2 m-2 p-4 border-gray-900\">\n        Pessoas\n      </h5>\n      <div class=\"flex justify-center items-center pt-10\">\n        <div class=\"w-full p-4 sm:p-6 md:p-8 dark:bg-gray-800\">\n          <div class=\"w-full\">\n            <div class=\"mb-4 flex items-center justify-between\">\n              <input\n                type=\"text\"\n                v-model=\"searchQuery\"\n                class=\"py-2 px-3 w-1/3 rounded border\"\n       
{"ast":null,"code":"import VendasService from '@/services/api/vendas/VendasService';\nimport { Bar } from 'vue-chartjs';\nimport { Chart as ChartJS, Title, Tooltip, Legend, BarElement, CategoryScale, LinearScale } from 'chart.js';\nChartJS.register(Title, Tooltip, Legend, BarElement, CategoryScale, LinearScale);\nconst meses = ['cliente', 'cliente', 'cliente', 'cliente', 'cliente', 'cliente', 'cliente', 'cliente', 'cliente', 'cliente', 'cliente'];\nexport default {\n  props: ['modelValue', 'initialPeople'],\n  emits: ['update:modelValue'],\n  name: 'BarChart',\n  components: {\n    Bar\n  },\n  data() {\n    return {\n      selectedPeople: this.modelValue,\n      vendas: [],\n      loading: false,\n      chartData: {\n        labels: meses,\n        datasets: [{\n          label: 'Total quantidade de vendas',\n          backgroundColor: '#87CEFA'\n        }]\n      },\n      chartOptions: {\n        responsive: true,\n        indexAxis: 'y'\n      }\n    };\n  },\n  watch: {\n    modelValue(newValue) {\n      this.selectedPeople = newValue;\n    },\n    selectedPeople(newValue) {\n      this.$emit('update:modelValue', newValue);\n    }\n  },\n  methods: {\n    async getAll() {\n      try {\n        const params = {\n          all: '0',\n          agrupar_cliente: '1'\n        };\n        this.loading = true;\n        const response = await VendasService.getAll(params);\n        this.vendas = response;\n        this.chartData.datasets[0].data = this.vendas.total_preco;\n        console.log(this.vendas);\n      } catch (error) {\n        console.error('Erro ao obter pessoas:', error);\n      } finally {\n        this.loading = false;\n      }\n    },\n    emitPeople() {\n      this.$emit('update:modelValue', this.selectedPeople);\n      this.$emit('people-selected', this.selectedPeople);\n    },\n    shouldApplyFilter() {\n      return this.selectedPeople;\n    },\n    buildParams() {\n      const params = {};\n      if (this.selectedPeople) {\n        params.pessoa_id = this.selectedPeople;\n      }\n      return params;\n    }\n  },\n  watch: {\n    selectedPeople() {\n      if (this.shouldApplyFilter()) {\n        this.getAll();\n      }\n    }\n  },\n  mounted() {\n    if (!this.initialPeople) {\n      this.getAll();\n    }\n  }\n};","map":{"version":3,"names":["VendasService","Bar","Chart","ChartJS","Title","Tooltip","Legend","BarElement","CategoryScale","LinearScale","register","meses","props","emits","name","components","data","selectedPeople","modelValue","vendas","loading","chartData","labels","datasets","label","backgroundColor","chartOptions","responsive","indexAxis","watch","newValue","$emit","methods","getAll","params","all","agrupar_cliente","response","total_preco","console","log","error","emitPeople","shouldApplyFilter","buildParams","pessoa_id","mounted","initialPeople"],"sources":["/home/brenomatheus/Documentos/Teste Pratico DEV Junior/frontend/src/pages/Home/components/Venda/components/ClientesChartComponent.vue"],"sourcesContent":["<template>\n  <div class=\"pt-6\">\n    <h5 class=\"text-xl font-bold pb-2\">Total de Vendas</h5>\n    <Bar id=\"my-chart-id\" :options=\"chartOptions\" :data=\"chartData\" />\n  </div>\n</template>\n\n<script>\nimport VendasService from '@/services/api/vendas/VendasService';\nimport { Bar } from 'vue-chartjs';\nimport {\n  Chart as ChartJS,\n  Title,\n  Tooltip,\n  Legend,\n  BarElement,\n  CategoryScale,\n  LinearScale,\n} from 'chart.js';\n\nChartJS.register(\n  Title,\n  Tooltip,\n  Legend,\n  BarElement,\n  CategoryScale,\n  LinearScale\n);\n\nconst meses = [\n  'cliente',\n  'cliente',\n  'cliente',\n  'cliente',\n  'cliente',\n  'cliente',\n  'cliente',\n  'cliente',\n  'cliente',\n  'cliente',\n  'cliente',\n  \n];\n\nexport default {\n  props: ['modelValue', 'initialPeople'],\n  emits: ['update:modelValue'],\n\n  name: 'BarChart',\n  components: { Bar },\n  data() {\n    return {\n      selectedPeople: this.modelValue,\n      vendas: [],\n      loading: false,\n      chartData: {\n        labels: meses,\n        datasets: [\n          {\n            label: 'Total quantidade de vendas',\n            backgroundColor: '#87CEFA',\n          },\n        ],\n      },\n      chartOptions: {\n        responsive: true,\n        indexAxis: 'y',\n      },\n    };\n  },\n  watch: {\n    modelValue(newValue) {\n      this.selectedPeople = newValue;\n    },\n    selectedPeople(newValue) {\n      this.$emit('update:modelValue', newValue);\n    },\n  },\n  methods: {\n    async getAll() {\n      try {\n        const params = { all: '0', agrupar_cliente: '1' };\n        this.loading = true;\n        const response = await VendasService.getAll(params);\n        this.vendas = response;\n\n        this.chartData.datasets[0].data = this.vendas.total_preco;\n        console.log(this.vendas)\n      } catch (error) {\n        console.error('Erro ao obter pessoas:', error);\n      } finally {\n        this.loading = false;\n      }\n    },\n    emitPeople() {\n      this.$emit('update:modelValue', this.selectedPeople);\n      this.$emit('people-selected', this.selectedPeople);\n    },\n    shouldApplyFilter() {\n      return this.selectedPeople;\n    },\n\n    buildParams() {\n      const params = {};\n      if (this.selectedPeople) {\n        params.pessoa_id = this.selectedPeople;\n      }\n      return params;\n    },\n  },\n  watch: {\n    selectedPeople() {\n      if (this.shouldApplyFilter()) {\n        this.getAll();\n      }\n    },\n  },\n  mounted() {\n    if (!this.initialPeople) {\n      this.getAll();\n    }\n  },\n};\n</script>\n"],"mappings":"AAQA,OAAOA,aAAY,MAAO,qCAAqC;AAC/D,SAASC,GAAE,QAAS,aAAa;AACjC,SACEC,KAAI,IAAKC,OAAO,EAChBC,KAAK,EACLC,OAAO,EACPC,MAAM,EACNC,UAAU,EACVC,aAAa,EACbC,WAAW,QACN,UAAU;AAEjBN,OAAO,CAACO,QAAQ,CACdN,KAAK,EACLC,OAAO,EACPC,MAAM,EACNC,UAAU,EACVC,aAAa,EACbC,WACF,CAAC;AAED,MAAME,KAAI,GAAI,CACZ,SAAS,EACT,SAAS,EACT,SAAS,EACT,SAAS,EACT,SAAS,EACT,SAAS,EACT,SAAS,EACT,SAAS,EACT,SAAS,EACT,SAAS,EACT,SAAS,CAEV;AAED,eAAe;EACbC,KA